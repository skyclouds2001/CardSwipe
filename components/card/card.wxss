@import "../../colorui/main.wxss";
@import "../../colorui/icon.wxss";
.card{
    width: 100%;
    height: 740rpx;
    background-color: #ffffff;
    box-sizing: border-box;
    border: 3rpx solid rgb(83, 201, 248);
    box-shadow: 2 10rpx 5rpx #999999;
    border-radius: 10rpx;
    margin-top: 50rpx;
}

.card .top{
    width: 100%; 
}
.card .bottom{
    position: relative;
    width: 100%; 
    padding: 20rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}
.text1{
    background-color: #f1f1f1; 
}
.one{
    position: relative;
}
.xing{
    position: absolute;
    right: 40rpx;
    bottom: 40rpx;
    width: 10%;
    height: 20px;
}
.xing image{
    width: 64rpx;
    height: 64rpx;
}
.progress-box{
    width: 105%;
    margin-left: -7px;
    margin-top: 7px;
    
}
